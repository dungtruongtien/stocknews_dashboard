{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "indent": [1, 2],
        "comma-dangle": [2, "never"],
        "global-require": 0,
        "new-cap": 0,
        "consistent-return": 1,
        "max-len": ["error", 120],
        "no-param-reassign": 0
    }
}
